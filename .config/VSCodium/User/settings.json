{
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.fontFamily": "ComicShannsMono Nerd Font Mono",
    "window.zoomLevel": 1,
    "editor.fontLigatures": true,
    "editor.mouseWheelZoom": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "workbench.startupEditor": "none",
    "files.autoSave": "afterDelay",
    "editor.formatOnSave": true,
    "editor.rulers": [
        100
    ],
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "redhat.telemetry.enabled": false,
    "editor.selectionClipboard": false,
    "yaml.format.printWidth": 100,
    "diffEditor.ignoreTrimWhitespace": false,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "editor.tabSize": 2,
    "diffEditor.useInlineViewWhenSpaceIsLimited": false,
    "editor.minimap.enabled": false,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "notebook.insertFinalNewline": true,
    "peacock.affectEditorGroupBorder": true,
    "peacock.affectPanelBorder": true,
    "peacock.affectSideBarBorder": true,
    "peacock.affectStatusAndTitleBorders": true,
    "peacock.affectTabActiveBorder": true,
    "peacock.affectDebuggingStatusBar": true,
    "peacock.surpriseMeOnStartup": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "files.autoSaveWorkspaceFilesOnly": true,
    "files.autoSaveWhenNoErrors": true,
    "editor.fontSize": 16,
    "clangd.path": "clangd-18",
    "terminal.integrated.showExitAlert": false,
    "editor.stickyScroll.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": true,
    "extensions.autoUpdate": false,
    "update.mode": "start",
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.enablePersistentSessions": false,
    "debug.terminal.clearBeforeReusing": true,
    "window.restoreWindows": "none"
}
